{
  "@context": {
    "dcterms": "http://purl.org/dc/terms/",
    "dcmitype": "http://purl.org/dc/dcmitype/",
    "bibo": "http://purl.org/ontology/bibo/",
    "schema": "https://schema.org/",
    "skos": "http://www.w3.org/2004/02/skos/core#",
    "prov": "http://www.w3.org/ns/prov#",
    "locn": "http://www.w3.org/ns/locn#",
    "wgs84": "http://www.w3.org/2003/01/geo/wgs84_pos#",
    "ccd": "https://comisioncorograficadigital.github.io/ccd-ontology/ccd.owl#",

    "id": "dcterms:identifier",
    "title": "dcterms:title",
    "shortTitle": "bibo:shortTitle",
    "description": "dcterms:description",
    "language": "dcterms:language",
    "subject": {"@id": "dcterms:subject", "@type": "@id"},
    "type": {"@id": "dcterms:type", "@type": "@id"},
    "created": "dcterms:created",
    "issued": "dcterms:issued",
    "date": "dcterms:date",
    "temporal": "dcterms:temporal",
    "spatial": {"@id": "dcterms:spatial", "@type": "@id"},
    "mentions": {"@id": "schema:mentions", "@type": "@id"},
    "location": {"@id": "locn:location", "@type": "@id"},
    "lat": "wgs84:lat",
    "long": "wgs84:long",
    "url": {"@id": "schema:url", "@type": "@id"},
    "isPartOf": {"@id": "dcterms:isPartOf", "@type": "@id"},
    "hasPart": {"@id": "dcterms:hasPart", "@type": "@id"},
    "isVersionOf": {"@id": "dcterms:isVersionOf", "@type": "@id"},
    "hasVersion": {"@id": "dcterms:hasVersion", "@type": "@id"},
    "wasDerivedFrom": {"@id": "prov:wasDerivedFrom", "@type": "@id"},
    "wasRevisionOf": {"@id": "prov:wasRevisionOf", "@type": "@id"},
    "used": {"@id": "prov:used", "@type": "@id"},
    "references": {"@id": "dcterms:references", "@type": "@id"},
    "source": "dcterms:source",
    "relation": {"@id": "dcterms:relation", "@type": "@id"}
